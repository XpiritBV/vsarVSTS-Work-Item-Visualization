{
  "name": "work-item-visualization",
  "version": "1.0.0",
  "description": "Microsoft DevLabs Work Item Visualization extension",
  "keywords": [
    "vsts",
    "tfs"
  ],
  "scripts": {
    "initdev": "typings install",
    "copyfiles": "copyfiles -f node_modules/vss-web-extension-sdk/lib/VSS.SDK.js node_modules/jquery/dist/jquery.min.js lib",
    "copyVssTypings": "copyfiles -f node_modules/vss-web-extension-sdk/typings/*.d.ts typings/vss",
    "setup": "npm run initdev && npm run copyfiles && npm run copyVssTypings",
    "package":"tfx extension create "
  },
  "author": "ALM Rangers",
  "license": "MIT",
  "devDependencies": {

      "grunt": "1.0.1",
    "grunt-exec": "1.0.1",
  
    "copyfiles": "1.0.0",
    "jquery": "2.2.2",
    "requirejs": "2.1.22",
    "tfx-cli": "0.3.45",
    "typings": "2.1.0",
    "vss-web-extension-sdk": "1.106.0",
    "tsd": "0.6.5",
    "typescript": "2.1.6",
    "grunt-ts":"6.0.0-beta.11",
    "grunt-contrib-concat": "1.0.1",
    "grunt-contrib-uglify": "2.1.0",
    "grunt-contrib-jasmine":"1.1.0",
    "cytoscape": "2.7.10",
    "cytoscape-dagre": "1.3.0",
    "cytoscape-navigator": "1.1.3",
    "cytoscape-context-menus": "2.0.3",
    "cytoscape-view-utilities": "1.0.6"
  }
}
