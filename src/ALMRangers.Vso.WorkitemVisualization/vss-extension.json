{
  "manifestVersion": 1,
  "id": "WorkItemVisualization",
  "version": "1.3.0",
  "name": "Work Item Visualization",
  "description": "Visualize relationships between work items from within the work item form.",
  "public": true,
  "publisher": "ms-devlabs",
  "scopes": [ "vso.work", "vso.code", "vso.test", "vso.profile" ],
  "icons": {
    "default": "images/WitVizShortIcon.png"
  },
  "categories": [
    "Plan and track"
  ],
  "tags": [
    "work item",
    "relationship relationships",
    "link links",
    "visualize visualization"
  ],
    "links": {
    "home": {
      "uri": "http://aka.ms/vsoext10"
    },
    "support": {
      "uri": "http://aka.ms/vsarcontact"
    },
    "issues": {
      "uri": "http://aka.ms/vsoext10"
    }
  },
  "branding": {
    "color": "rgb(51,65,133)",
    "theme": "dark"
  },
  "contributions": [
    {
      "id": "work-item-visualization-dialog",
      "type": "ms.vss-web.control",
      "targets": [ ".controls" ],
      "properties": {
        "uri": "WorkitemVisualization.html"
      }
    },
    {
      "id": "visualize-work-item-action",
      "type": "ms.vss-web.action",
      "description": "Shows the target properties for menu actions for work items",
      "targets": [
        "ms.vss-work-web.work-item-context-menu"
      ],
      "properties": {
        "text": "Visualize",
        "title": "Visualize this workitem",
        "toolbarText": "Visualize",
        "icon": "images/WitVizShortIcon-16x16.png",
        "uri": "VisualizeWitButton.html",
        "registeredObjectId": "visualize-work-item-action"
      }
    },
    {
      "id": "work-item-visualization-find-wit-dialog",
      "type": "ms.vss-web.control",
      "targets": [ ".controls" ],
      "properties": {
        "uri": "FindWitDialog.html"
      }
    },
    {
      "id": "work-item-visualization-add-edit-highlight-dialog",
      "type": "ms.vss-web.control",
      "targets": [ ".controls" ],
      "properties": {
        "uri": "AddEditHighlightDialog.html"
      }
    },
    {
      "id": "work-item-visualization-print-graph-dialog",
      "type": "ms.vss-web.control",
      "targets": [ ".controls" ],
      "properties": {
        "uri": "PrintGraph.html"
      }
    }
  ],
  "content": {
    "details": {
      "path": "overview.md"
    },
    "license": {
      "path": "marketplace/mp_terms.md"
    }
  },
  "files": [
    {
      "path": "css",
      "addressable": true
    },
    {
      "path": "images",
      "addressable": true
    },
    {
      "path": "scripts",
      "addressable": true
    },
    {
      "path": "AddEditHighlightDialog.html",
      "addressable": true
    },
    {
      "path": "FindWitDialog.html",
      "addressable": true
    },
    {
      "path": "PrintGraph.html",
      "addressable": true
    },
    {
      "path": "VisualizeWitButton.html",
      "addressable": true
    },
    {
      "path": "WorkitemVisualization.html",
      "addressable": true
    },
    {
      "path": "ThirdPartyNotice.txt",
      "addressable": true
    }
  ],
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ]
}
